{
  "version": 2,
  "routes": [
	{ "handle": "filesystem" },
	{ "src": "/", "dest": "/index.html"},
	{ "src": "/favicon.ico", "dest": "/favicon.ico"},
	{ "src": "/tootssocial/(.*)", "dest": "https://toots.s3.nl-ams.scw.cloud/$1", "headers": { "cache-control": "public, max-age=63072000"}},
	{ "src": "/(accounts|cache|custom_emojis|media_attachments|preview_cards|site_uploads)/(.*)", "dest": "https://toots.s3.nl-ams.scw.cloud/$1/$2", "headers": { "cache-control": "public, max-age=63072000"}},
	{ "src": "/7ed/social/(.*)", "dest": "https://objectstorage.ap-seoul-1.oraclecloud.com/n/cnbr0vgx02y9/b/social/o/7ed/social/$1", "headers": { "cache-control": "public, max-age=63072000"}},
	{ "src": "/tgfiles/(.*)", "dest": "https://tgfiles.herokuapp.com/$1", "headers": { "cache-control": "public, max-age=63072000"}},
	{ "src": "/random/(.*)", "dest": "https://uploadbeta.com/api/pictures/random/?key=BingEverydayWallpaperPicture&id=$1"},
	{ "src": "/(.*)", "dest": "https://cdn.statically.io/img/$1", "headers": { "cache-control": "public, max-age=63072000"}},
	{ "src": "/(.*)", "status": 404, "dest": "/404.jpg" }
  ]
}
