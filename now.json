{
  "version": 2,
  "routes": [
	{ "handle": "filesystem" },
	{ "src": "/", "dest": "/index.html"},
	{ "src": "/favicon.ico", "dest": "/favicon.ico"},
	{ "src": "/(.*)", "dest": "https://cdn.statically.io/img/$1", "headers": { "cache-control": "public, max-age=63072000"}},
	{ "src": "/(.*)", "status": 404, "dest": "/404.jpg" }
  ]
}
